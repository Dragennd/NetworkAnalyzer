<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Border Colors-->
    <SolidColorBrush x:Key="BorderBrush">#FF6B6B78</SolidColorBrush>
    <Color x:Key="BorderColor">#FF6B6B78</Color>

    <SolidColorBrush x:Key="ShadowBrush">#FF1E1E21</SolidColorBrush>
    <Color x:Key="ShadowColor">#FF1E1E21</Color>

    <!--Background Colors-->
    <SolidColorBrush x:Key="MainWindowBackground">#FFFFFFFF</SolidColorBrush>
    <Color x:Key="MainWindowBackgroundColor">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="MainWindowMenu">#FFF5F5F5</SolidColorBrush>

    <SolidColorBrush x:Key="ContentHeaderBrush">#FFE5E5E5</SolidColorBrush>
    <Color x:Key="ContentHeaderColor">#FFE5E5E5</Color>

    <SolidColorBrush x:Key="ContentBrush">#FFE5E5E5</SolidColorBrush>
    <Color x:Key="ContentColor">#FFE5E5E5</Color>

    <SolidColorBrush x:Key="SelectedBrush">#FFE5E5E5</SolidColorBrush>
    <Color x:Key="SelectedColor">#FFE5E5E5</Color>

    <SolidColorBrush x:Key="IsMouseOverBrush">#FFE5E5E5</SolidColorBrush>
    <Color x:Key="IsMouseOverColor">#FFE5E5E5</Color>

    <SolidColorBrush x:Key="TintedContentBrush">#FFE5E5E5</SolidColorBrush>
    <Color x:Key="TintedContentColor">#FFE5E5E5</Color>

    <SolidColorBrush x:Key="GlyphBrush">#FF6B6B78</SolidColorBrush>
    <Color x:Key="GlyphColor">#FF6B6B78</Color>

    <!--Foreground Colors-->
    <SolidColorBrush x:Key="TextBrush">#FF616161</SolidColorBrush>
    <Color x:Key="TextColor">#FF616161</Color>

    <SolidColorBrush x:Key="MenuTextBrush">#FF616161</SolidColorBrush>
    <Color x:Key="MenuTextColor">#FF616161</Color>

    <SolidColorBrush x:Key="DisabledBrush">#FFA2B1C0</SolidColorBrush>
    <Color x:Key="DisabledColor">#FFA2B1C0</Color>

    <SolidColorBrush x:Key="InactiveBrush">#FF50505C</SolidColorBrush>
    <Color x:Key="InactiveColor">#FF50505C</Color>

    <SolidColorBrush x:Key="OnMouseOverBrush">#FFFF8C00</SolidColorBrush>
    <Color x:Key="OnMouseOverColor">#FFFF8C00</Color>

    <SolidColorBrush x:Key="CaretBrush">#FF616161</SolidColorBrush>
    <Color x:Key="CaretColor">#FF616161</Color>
</ResourceDictionary>